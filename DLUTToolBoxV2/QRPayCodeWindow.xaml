<Window x:Class="DLUTToolBox_V2.QRPayCodeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DLUTToolBox_V2"
        mc:Ignorable="d"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        xmlns:controls="https://handyorg.github.io/handycontrol"
        Title="QRCodePayWindow" Height="700" Width="600" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <Image Width="580" Height="580" Margin="10" x:Name="QRCode"/>
            <Button Content="返回" FontSize="26" Width="200" Height="50" Margin="10" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
